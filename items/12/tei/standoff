         <note xml:id="nonenglish_198" type="description">
            <term id="nonenglish_198-term">abita nel</term>
            <gloss id="nonenglish_198-gloss" target="#nonenglish_198-term">
               <p>lives in (Italian)</p>
            </gloss>
         </note>
         <note xml:id="nonenglish_199" type="description">
            <term id="nonenglish_199-term">dalla parte di S Apposboli = ultimo piano</term>
            <gloss id="nonenglish_199-gloss" target="#nonenglish_199-term">
               <p>on the side of S Apostoli = top floor (Italian)</p>
            </gloss>
         </note>
